{
    "name": "Gentle Reminder",
    "version": "1.0",
    "description": "A gentle reminder that shows when loading certain websites",
    
    "permissions": [
        "notifications"
    ],

    "content_scripts": [
        {
        "matches": ["https://staging.theculturetrip.com/*"],
        "css": ["styles.css"],
        "js": ["scripts.js"]
        }
    ],
    "manifest_version": 2
}